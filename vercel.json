{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist/erp-ics-front/browser"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/proxy/(.*)",
      "dest": "http://institutoclima128986.protheus.cloudtotvs.com.br:4050//$1"
    },
    {
      "src": "/(.*\\.(js|css|ico|png|jpg|jpeg|gif|svg|woff|woff2|ttf|eot))",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}